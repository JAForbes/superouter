    "mithril",
    "sum-type",
    "union-type",
    "fp",
    "react",
    "react-router",
    "router",
    "express",
    "minimatch",
    "path-to-regexp"
  ],
  "devDependencies": {
    "rollup": "^3.21.1",
    "typescript": "^5.0.4",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-swc": "^0.1.0",
    "@types/mithril": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
