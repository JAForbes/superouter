{
  "name": "superouter",
  "version": "0.0.3",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:test": "nyc --reporter=lcov tape \"./test/*.js\" | tap-spec",
    "test:check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "test:report-coverage": "nyc report",
    "predist": "rm -fr dist && mkdir dist",
    "dist": "npm-run-all --parallel dist:*",
    "dist:fold": "browserify lib/index.js --standalone superouter | uglifyjs -mc > dist/superouter.min.js",
    "release": "git push && npm version minor && npm publish",
    "--preversion": "npm test"
  },
  "author": "james.a.forbes@gmail.com <=> (http://james-forbes.com)",
  "license": "MIT",
  "repository": "git@gitlab.com:harth/superouter.git",
  "devDependencies": {
    "browserify": "^16.2.3",
    "eslint": "^5.8.0",
    "npm-run-all": "^4.1.3",
    "nyc": "^13.1.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1",
    "uglify-js": "^3.4.9"
  },
  "files": [
    "dist/*",
    "readme.md",
    "index.js"
  ]
}
